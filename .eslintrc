{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "plugins": ["babel"],
  "root": true,
  "rules": {
    "comma-dangle": ["error", "never"],
    "arrow-parens": ["error", "always"],
    "react/destructuring-assignment": "off",
    "jsx-a11y/anchor-is-valid": "off",
    "jsx-a11y/label-has-for": ["off", {
      "components": ["Label"],
      "required": {
        "every": ["nesting", "id"]
      },
      "allowChildren": false
    }]
  }
}